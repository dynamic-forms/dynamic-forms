{
  "name": "@dynamic-forms/common",
  "version": "8.0.0",
  "author": "dynamic-forms",
  "description": "dynamic-forms is an Angular project for dynamic forms based on JSON.",
  "keywords": [
    "angular",
    "forms",
    "reactive forms",
    "json",
    "json forms",
    "dynamic-forms"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dynamic-forms/dynamic-forms.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --no-webdriver-update",
    "e2e:prod": "ng e2e --no-webdriver-update --configuration=production",
    "e2e:azure": "ng e2e --no-webdriver-update --configuration=azure",
    "e2e:azure:prod": "ng e2e --no-webdriver-update --configuration=azure-production",
    "e2e:watch": "watch \"npm run e2e -- --dev-server-target=\" apps/demo/src apps/demo-e2e/src --wait=1",
    "build:core": "ng build dynamic-forms-core && npm run copy:styles:core",
    "build:bootstrap": "ng build dynamic-forms-bootstrap && npm run copy:styles:bootstrap",
    "build:material": "ng build dynamic-forms-material && npm run copy:styles:material",
    "build:libs": "npm run build:core && npm run build:bootstrap && npm run build:material",
    "build:demo": "ng build dynamic-forms-demo",
    "build:demo:prod": "ng build dynamic-forms-demo --prod --aot",
    "test:libs": "ng test dynamic-forms-libs --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:demo": "ng test dynamic-forms-demo --browsers=ChromeHeadless --watch=false",
    "cover:core": "ng test dynamic-forms-core --browsers=ChromeHeadless --watch=false --code-coverage",
    "cover:bootstrap": "ng test dynamic-forms-bootstrap --browsers=ChromeHeadless --watch=false --code-coverage",
    "cover:material": "ng test dynamic-forms-material --browsers=ChromeHeadless --watch=false --code-coverage",
    "cover:libs": "npm run cover:core && npm run cover:bootstrap && npm run cover:material",
    "doc:core": "cd libs/core && npx compodoc -c doc.json",
    "doc:bootstrap": "cd libs/bootstrap && npx compodoc -c doc.json",
    "doc:material": "cd libs/material && npx compodoc -c doc.json",
    "doc:libs": "npm run doc:core && npm run doc:bootstrap && npm run doc:material",
    "serve:core": "ng build dynamic-forms-core --watch",
    "serve:bootstrap": "ng build dynamic-forms-bootstrap --watch",
    "serve:material": "ng build dynamic-forms-material --watch",
    "serve:doc:core": "cd libs/core && npx compodoc -c doc.json --serve --watch",
    "serve:doc:bootstrap": "cd libs/bootstrap && npx compodoc -c doc.json --serve --watch",
    "serve:doc:material": "cd libs/material && npx compodoc -c doc.json --serve --watch",
    "build:localhost": "npm run lint && npm run build:libs && npm run cover:libs && npm run doc:libs && npm run build:demo -- --base-href=/v8/",
    "build:localhost:prod": "npm run lint && npm run build:libs && npm run cover:libs && npm run doc:libs && npm run build:demo:prod -- --base-href=/v8/",
    "publish:core": "cd dist/v8/@dynamic-forms/core && npm publish",
    "publish:bootstrap": "cd dist/v8/@dynamic-forms/bootstrap && npm publish",
    "publish:material": "cd dist/v8/@dynamic-forms/material && npm publish",
    "publish:libs": "npm run publish:core && npm run publish:bootstrap && npm run publish:material",
    "clean:styles:core": "rimraf dist/v8/@dynamic-forms/core/assets/scss",
    "clean:styles:bootstrap": "rimraf dist/v8/@dynamic-forms/bootstrap/assets/scss",
    "clean:styles:material": "rimraf dist/v8/@dynamic-forms/material/assets/scss",
    "clean:styles:libs": "npm run clean:styles:core && npm run clean:styles:bootstrap && npm run clean:styles:material",
    "copy:styles:core": "npm run clean:styles:core && cd libs/core/assets && copyfiles \"./**/*.scss\" ../../../dist/v8/@dynamic-forms/core/assets",
    "copy:styles:bootstrap": "npm run clean:styles:bootstrap && cd libs/bootstrap/assets && copyfiles \"./**/*.scss\" ../../../dist/v8/@dynamic-forms/bootstrap/assets",
    "copy:styles:material": "npm run clean:styles:material && cd libs/material/assets && copyfiles \"./**/*.scss\" ../../../dist/v8/@dynamic-forms/material/assets",
    "copy:styles:libs": "npm run copy:styles:core && npm run copy:styles:bootstrap && npm run copy:styles:material"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngxs/store": "^3.7.1",
    "bootstrap": "^4.6.0",
    "marked": "^2.0.3",
    "rxjs": "~6.6.7",
    "tslib": "^1.14.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.7.4",
    "@types/jasminewd2": "~2.0.9",
    "@types/marked": "^2.0.2",
    "@types/node": "~15.3.0",
    "@types/uuid": "^8.3.0",
    "codelyzer": "~6.0.2",
    "copyfiles": "^2.4.1",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^5.7.1",
    "protractor": "~7.0.0",
    "protractor-beautiful-reporter": "^1.3.6",
    "rimraf": "^3.0.2",
    "ts-node": "~9.1.1",
    "tsickle": ">=0.37.0",
    "tslint": "~6.1.3",
    "typescript": "~3.5.3",
    "watch": "^1.0.2"
  }
}
